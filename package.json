{
    "name": "nexo-css",
    "version": "0.1.3",
    "description": "CSS framework based on modern technologies: Flexbox with BEM and Atomic Design methodologies.",
    "main": "index.js",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --watch --watch-poll --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "hot": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --inline --hot --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kaligari/nexo-css.git"
    },
    "keywords": [
        "css",
        "bem",
        "atomic",
        "design",
        "bulma",
        "bemskel",
        "flex"
    ],
    "author": "Sebastian KopeÄ‡",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/kaligari/nexo-css/issues"
    },
    "homepage": "https://github.com/kaligari/nexo-css#readme",
    "devDependencies": {
        "cross-env": "^7.0.2",
        "laravel-mix": "^5.0.4",
        "prismjs": "^1.20.0",
        "sass": "^1.26.3",
        "sass-loader": "^8.0.2",
        "vue": "^2.6.11",
        "vue-prism-component": "^1.2.0",
        "vue-router": "^3.1.6",
        "vue-template-compiler": "^2.6.11",
        "vuex": "^3.3.0"
    },
    "dependencies": {}
}
